* {
    background: #404040; /*#555*/
    color: #e6e6e6; /*#e6e6e6*/
}
QPushButton {
    outline: none;
    border: none;
    background-color: transparent;
    color: #888;
}
QPushButton:hover {
    color: #e6e6e6;
}
QPushButton:pressed {
    background-color: transparent;
    border:none;
}
QPushButton:focus {
    color: #e6e6e6;
}
QListWidget {
    font-weight:400;
}
QLineEdit {
    background: #333;
    border-radius: 5px;
    color: #bbb;
    font-weight: 400;
}